# ZenCube Environment Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control

# API Keys (add your keys here when implementing external integrations)
# OPENAI_API_KEY=your_openai_api_key_here
# ANTHROPIC_API_KEY=your_anthropic_api_key_here
# GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# Database Configuration (if needed)
# DATABASE_URL=postgresql://user:password@localhost:5432/zencube
# REDIS_URL=redis://localhost:6379/0

# Application Settings
# ENV=development
# DEBUG=true
# LOG_LEVEL=INFO

# External Service URLs
# WEBHOOK_URL=https://your-webhook-service.com/endpoint
# MONITORING_ENDPOINT=https://your-monitoring-service.com/api

# Security Settings
# SECRET_KEY=your_secret_key_here
# JWT_SECRET=your_jwt_secret_here
# ENCRYPTION_KEY=your_encryption_key_here

# Feature Flags
# ENABLE_MONITORING=true
# ENABLE_ALERTS=true
# ENABLE_PROMETHEUS=false
